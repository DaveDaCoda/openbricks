#!/bin/sh

. config/options

$SCRIPTS/unpack linux || exit 1
$SCRIPTS/build toolchain || exit 1

CWD=`pwd`
export CROSS_COMPILE=$TARGET_PREFIX
export ARCH=$TARGET_ARCH

cd $BUILD/rt2400*/Module && \
make KERNEL_SOURCES=$KERNEL \
     KERNEL_HEADERS=$KERNEL/include \
     PATCHLEVEL=6 && \
cd "$CWD" && \
exit 0

cd "$CWD"
exit 1
