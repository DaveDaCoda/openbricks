#!/bin/sh

. config/options

$SCRIPTS/build toolchain || exit 1

CWD=`pwd`

cd $BUILD/pciutils*/lib && \
./configure && \
make && \
mkdir -p $LIB_PREFIX/lib $LIB_PREFIX/include/pci && \
cp config.h header.h internal.h pci.h $LIB_PREFIX/include/pci && \
cp libpci.a $LIB_PREFIX/lib && \
cd "$CWD" && \
exit 0

cd "$CWD"
exit 1
