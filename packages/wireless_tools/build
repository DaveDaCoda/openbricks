#!/bin/sh

. config/options

$SCRIPTS/build toolchain || exit 1
$SCRIPTS/build linux || exit 1

make -C $BUILD/wireless_tools* \
     PREFIX=/usr \
     KERNEL_SRC=$KERNEL \
     CC=$CC \
     AR=$AR \
     RANLIB=$RANLIB \
     CFLAGS="$CFLAGS -I." \
     BUILD_STATIC="yes" \
     iwconfig && \
exit 0

exit 1
