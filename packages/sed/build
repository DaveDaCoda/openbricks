#!/bin/sh

. config/options

$SCRIPTS/install make || exit 1
$SCRIPTS/install ccache || exit 1

CWD=`pwd`
unset INSTALL
export CC=$HOST_CC
export CFLAGS="-O2"
export LDFLAGS="-s"

cd $BUILD/sed* && \
./configure --prefix=$ROOT/$TOOLCHAIN \
            --disable-nls && \
make && \
cd $CWD && \
exit 0

cd $CWD
exit 1
