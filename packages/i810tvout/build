#!/bin/sh

. config/options toolchain

$SCRIPTS/build toolchain || exit 1
$SCRIPTS/build pciutils || exit 1

INCLUDE="-I`ls -d $ROOT/$BUILD/pciutils*/`"
LIBS="-L`ls -d $ROOT/$BUILD/pciutils*/lib`"

make -C $BUILD/i810tvout \
     CC=$CC \
     CFLAGS="$CFLAGS $INCLUDE $LIBS" \
     i810tvout && \
exit 0

exit 1
