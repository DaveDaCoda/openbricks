#!/bin/sh

. config/options

$SCRIPTS/build toolchain
$SCRIPTS/build fuse
$SCRIPTS/build talloc

cd $BUILD/$1*
./configure --host=$TARGET_NAME \
            --build=$HOST_NAME \
            --with-external-talloc \
            --without-external-libupnp \
            --without-libiconv-prefix \
            --disable-debug
make
