#!/bin/sh

. config/options

$SCRIPTS/build toolchain || exit 1

unset INSTALL

cd $BUILD/libungif* && \
./configure --host=$TARGET_NAME \
            --build=$HOST_NAME \
            --prefix=$LIB_PREFIX \
            --disable-shared \
            --without-x && \
make -C lib && \
make -C lib install && \
exit 0

exit 1
