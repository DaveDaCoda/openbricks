#!/bin/sh

. config/options

$SCRIPTS/build toolchain || exit 1

unset INSTALL
CWD=`pwd`

cd $BUILD/libungif* && \
./configure --host=$TARGET_NAME \
            --build=$HOST_NAME \
            --prefix=$LIB_PREFIX \
            --disable-shared \
            --without-x && \
make -C lib && \
make -C lib install && \
cd "$CWD" && \
exit 0

cd "$CWD"
exit 1
