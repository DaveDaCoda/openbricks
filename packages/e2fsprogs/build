#!/bin/sh

. config/options

$SCRIPTS/build toolchain

cd $BUILD/e2fsprogs*
./configure --host=$TARGET_NAME \
            --build=$HOST_NAME \
            --with-ldopts="$LDFLAGS" \
            --disable-shared \
            --disable-resizer \
            --disable-imager \
            --disable-debugfs \
            --disable-swapfs
make libs
make mke2fs -C misc
