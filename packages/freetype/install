#!/bin/sh

. config/options


SO=`ls -d $BUILD/freetype*`/libfreetype.so.6
if [ ! -f $SO -o $BUILD/freetype*/build/lib/libfreetype.a -nt $SO \
     -o $BUILD/MPlayer*/mplayer -nt $SO ]; then
  $SCRIPTS/libstrip $SO $BUILD/freetype*/build/lib/libfreetype.a $BUILD/MPlayer*/mplayer
fi
mkdir -p $INSTALL/usr/lib
cp $SO $INSTALL/usr/lib || exit 1
