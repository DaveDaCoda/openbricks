#!/bin/sh

. config/options

$SCRIPTS/build toolchain || exit 1

make -C $BUILD/libpcd* \
     CC=$CC \
     AR=$AR \
     RANLIB=$RANLIB \
     libpcd.a && \
mkdir -p $LIB_PREFIX/lib $LIB_PREFIX/include && \
cp $BUILD/libpcd*/pcd.h $LIB_PREFIX/include && \
cp $BUILD/libpcd*/libpcd.a $LIB_PREFIX/lib && \
exit 0

exit 1
