#!/bin/sh

. config/options

$SCRIPTS/build toolchain
$SCRIPTS/build cpufrequtils

export ac_cv_func_malloc_0_nonnull=yes
export ac_cv_lib_cpufreq_cpufreq_cpu_exists=yes
export LDADD="-lsysfs"

cd $BUILD/$1*
./configure --host=$TARGET_NAME \
            --build=$HOST_NAME \
            --prefix= \
            --exec-prefix=/usr \
            --disable-static
make
