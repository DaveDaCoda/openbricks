#!/bin/sh

. config/options toolchain

$SCRIPTS/unpack linux || exit 1
$SCRIPTS/build toolchain || exit 1

CWD=`pwd`
export CROSS_COMPILE=$TARGET_PREFIX
export ARCH=$TARGET_ARCH

cd $BUILD/RT2500-*/STA/Module && \
cp -f 2.6.x/Makefile . && \
make -C $KERNEL SUBDIRS=$PWD modules && \
cd "$CWD" && \
exit 0

cd "$CWD"
exit 1
