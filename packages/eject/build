#!/bin/sh

. config/path

$SCRIPTS/build uClibc || exit 1


export UCLIBC_GCC_DLOPT="-Wl,--dynamic-linker,/lib/ld-uClibc.so.0"
CWD=`pwd`

cd "`ls -d $BUILD/eject-*`" && \
./configure && \
make CC="`ls $ROOT/$BUILD/uClibc-*/build/usr/bin/gcc`" && \
strip eject && \
cd "$CWD" && \
exit 0

cd "$CWD"
exit 1
