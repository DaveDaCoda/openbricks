#!/bin/sh

. config/options

$SCRIPTS/install ccache || exit 1

setup_toolchain host
unset INSTALL

cd $BUILD/binutils* && \
./configure --target=$TARGET_NAME \
            --prefix=$ROOT/$TOOLCHAIN \
            --with-sysroot=$SYSROOT_PREFIX \
            --disable-nls && \
make && \
exit 0

exit 1
