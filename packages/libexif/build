#!/bin/sh

. config/options

$SCRIPTS/build toolchain || exit 1

unset INSTALL

cd $BUILD/libexif* && \
./configure --host=$TARGET_NAME \
            --build=$HOST_NAME \
            --prefix=$LIB_PREFIX \
            --disable-nls && \
make -C libexif && \
make -C libexif install && \
exit 0

exit 1
