#!/bin/sh

. config/options

$SCRIPTS/build uClibc
$SCRIPTS/build busybox
$SCRIPTS/build iscd
$SCRIPTS/build fixcodec

STAMP=.stamps/initrd/build

for p in uClibc busybox iscd fixcodec; do
  test .stamps/$p/build -nt $STAMP && rm -f $STAMP
done

for f in console dev hotplug linuxrc nosystem ro rw; do
  test $PACKAGES/initrd/$f -nt $STAMP && rm -f $STAMP
done

exit 0
